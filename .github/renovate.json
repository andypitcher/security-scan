{
  "extends": [
    "github>rancher/renovate-config//rancher-main#release"
  ],
  "baseBranchPatterns": [
    "main"
  ],
  "prHourlyLimit": 4,
  "packageRules": [
    {
      "description": "Disable non-security bumps for backporting branches",
      "enabled": false,
      "matchBaseBranches": [
        "/^release/v.*/"
      ]
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true
}
